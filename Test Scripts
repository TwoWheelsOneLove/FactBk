Login; 
start_chrome()
go_to(server IP)
write("igor", into="Email")
write(“testPassword4”, into="Password")
click("Sign in")

Open Fact;
start_chrome()
go_to(server IP)
write("igor" into="Email")
write("testPassword4", into="Password")
click(“Sign in”)
click(“Marker”)

Share Fact;
start_chrome()
go_to(server IP)
write("igor", into="Email")
write(“testPassword4”, into="Password")
click("Sign in")
click("Marker")
click ("Share marker")

Get Directions;
start_chrome()
go_to(server IP)
write("igor", into="Email")
write(“testPassword4” into="Email")
click("Sign in")
click("Marker")
click("Directions marker")
